<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="registrar_create" path="/create">
        <default key="_controller">RegistrarBundle:Default:create</default>
    </route>

    <route id="registrar_create" path="/create" methods="GET|POST">
        <default key="_controller">RegistrarBundle:Default:create</default>
    </route>

    <route id="registrar_validate" path="/validate" methods="GET|POST">
        <default key="_controller">RegistrarBundle:Default:validate</default>
    </route>
</routes>
